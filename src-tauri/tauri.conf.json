{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gasolink-app",
  "version": "0.1.0",
  "identifier": "com.gasolink.gasolinkapp",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "gasolink-app",
        "resizable": false,
        "fullscreen": false,
        "maximized": true,
        "decorations": true,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {}
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
